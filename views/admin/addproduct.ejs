      <div class="form-container">
        <h2>ADD PRODUCT</h2>
        <form class="add-product" method="post" enctype="multipart/form-data" onsubmit="return validateForm() " >
          <div class="form-field">
            <label for="name">Product Name</label>
            <input type="text" id="name" name="name" placeholder="Enter product name" required oninput="validateField(this)">
            <br>
            <span class="field-error" id="name-error"></span>
          </div>
          <div class="form-field">
            <label for="price">Price</label>
            <input type="number" id="price" name="price" placeholder="Enter price" required oninput="validateField(this)">
            <br>
            <span class="field-error" id="price-error"></span>
          </div>
          <div class="form-field">
            <label for="category">Category</label>
            <select id="category" name="category" required oninput="validateField(this)">
              <option disabled selected>Select category</option>
              <% for (let i=0; i < category.length; i++) { %>
              <option value="<%= category[i].category %>"><%= category[i].category %></option>
              <% } %>
            </select>
            <br>
            <span class="field-error" id="category-error"></span>
          </div>
          <div class="form-field">
            <label for="image-upload" class="custom-file-upload">
              <i class="fas fa-cloud-upload-alt"></i> Choose Image
            </label>
            <input type="file" id="image-upload" name="image" accept="image/*" multiple onchange="showSelectedImage(event)">
          
            <div id="selected-images-preview" class="image-preview"></div>
          </div>
          
          
          
          <div class="form-field">
            <label for="description">Product Description</label>
            <textarea id="description" name="description" placeholder="Enter product description" required
              oninput="validateField(this)"></textarea>
            <br>
            <span class="field-error" id="description-error"></span>
          </div>
          <div class="form-field">
            <label for="brand">Brand</label>
            <input type="text" id="brand" name="brand" placeholder="Enter brand" required oninput="validateField(this)">
            <br>
            <span class="field-error" id="brand-error"></span>
          </div>
          <div class="form-field">
            <label for="size">Size</label>
            <input type="text" id="size" name="size" placeholder="Enter size" required oninput="validateField(this)">
            <br>
            <span class="field-error" id="size-error"></span>
          </div>
          <div class="form-field">
            <label for="stock">Stock</label>
            <input type="number" id="stock" name="stock" placeholder="Enter stock quantity" required
              oninput="validateField(this)">
            <br>
            <span class="field-error" id="stock-error"></span>
          </div>
          <button class="submit-btn" type="submit">
            <span class="button__text">Submit</span>
            <i class="button__icon fas fa-chevron-right"></i>
          </button>
        </form>
      </div>

      <script>
        function validateForm() {
          // Resetting any previous error messages
          resetErrorMessages();
      
          // Validating each field
          var nameField = document.getElementById("name");
          if (nameField.value === "") {
            displayErrorMessage("name-error", "Please enter a product name.");
            return false;
          }
      
          var priceField = document.getElementById("price");
          if (priceField.value === "" || priceField.value < 0) {
            displayErrorMessage("price-error", "Please enter a valid price.");
            return false;
          }
      
          var categoryField = document.getElementById("category");
          if (categoryField.value === "") {
            displayErrorMessage("category-error", "Please select a category.");
            return false;
          }
      
          var imageField = document.getElementById("image-upload");
          if (imageField.value === "") {
            displayErrorMessage("image-error", "Please choose an image.");
            return false;
          }
      
          var descriptionField = document.getElementById("description");
          if (descriptionField.value === "") {
            displayErrorMessage("description-error", "Please enter a product description.");
            return false;
          }
      
          var brandField = document.getElementById("brand");
          if (brandField.value === "") {
            displayErrorMessage("brand-error", "Please enter a brand.");
            return false;
          }
          // Validate that only letters are entered in the brand field
          var brandRegex = /^[A-Za-z]+$/;
          if (!brandRegex.test(brandField.value)) {
            displayErrorMessage("brand-error", "Please enter only letters in the brand field.");
            return false;
          }
      
          var sizeField = document.getElementById("size");
          if (sizeField.value === "") {
            displayErrorMessage("size-error", "Please enter a size.");
            return false;
          }
      
          var stockField = document.getElementById("stock");
          if (stockField.value === "" || stockField.value < 0) {
            displayErrorMessage("stock-error", "Please enter a valid stock quantity.");
            return false;
          }
          // Validate that only numbers are entered in the stock field
          var stockRegex = /^[0-9]+$/;
          if (!stockRegex.test(stockField.value)) {
            displayErrorMessage("stock-error", "Please enter only numbers in the stock field.");
            return false;
          }
      
          // If all validations pass, the form can be submitted
          return true;
        }
      
        function displayErrorMessage(elementId, message) {
          var errorElement = document.getElementById(elementId);
          errorElement.innerText = message;
        }
      
        function resetErrorMessages() {
          var errorElements = document.getElementsByClassName("field-error");
          for (var i = 0; i < errorElements.length; i++) {
            errorElements[i].innerText = "";
          }
        }
      
        function validateField(inputField) {
          if (inputField.value !== "") {
            resetErrorMessages();
          }
        }
      
        function showSelectedImage(event) {
  const previewContainer = document.getElementById('selected-images-preview');
  previewContainer.innerHTML = '';

  const files = event.target.files;
  for (let i = 0; i < files.length; i++) {
    const file = files[i];
    const reader = new FileReader();

    reader.onload = function (event) {
      const imgElement = document.createElement('img');
      imgElement.src = event.target.result;
      imgElement.classList.add('selected-image');
      previewContainer.appendChild(imgElement);
    };

    reader.readAsDataURL(file);
  }
}


      </script>
